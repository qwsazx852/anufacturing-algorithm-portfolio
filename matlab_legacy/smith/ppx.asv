% % n=20;
% % chromosome=50;
% % GCT=1000;
% % Information=[142 34 140 214 121 279 50 282 129 175 97 132 107 132 69 169 73 231 120 186]
% 
% ee=[];
% bb=[];
% C=[];
% G=[];
% for c=1:2:chromosome
%     D=fix(rand(2,n)*2+1);
%     for e= 1:2
%         b1=a(c,:);
%         b2=a(c+1,:);
%         for t =1:n%%
%             
%             if D(e,t)==1
%                 C=[C,b1(1)];
%                 
%                 h=find(b2==b1(1));
%                 b1(1)=[];
%                 b2(h)=[];
%                 
%             else
%                 
%                 C=[C,b2(1)];
%                 j=find(b1==b2(1));
%                 b2(1)=[];
%                 b1(j)=[];
%             end
%             
%         end
%         
%        ee=[ee;C];
%        bb=[bb;LBss1(GCT,Information,C)]
%         C=[];
%     end
% end
function [ee,bb] = ppx(n,a,crossover,T1,T22)

ee=[];
bb=cell(100,2);
C=[];
G=[];
i=0
for c=1:2:length(a)
    if rand<crossover
        i=i+1
        D=fix(rand(2,n)*2+1);
        for e= 1:2
            i=i+1
            b1=a(c,:);
            b2=a(c+1,:);
            for t =1:n%%

                if D(e,t)==1
                    C=[C,b1(1)];

                    h=find(b2==b1(1));
                    b1(1)=[];
                    b2(h)=[];

                else

                    C=[C,b2(1)];
                    j=find(b1==b2(1));
                    b2(1)=[];
                    b1(j)=[];
                end

            end

            ee=[ee;C]
%             bb=[bb;LBss2(C,T1,T22),LBss3(C,T1,T22)];
            bb{i,1}=LBss2(C,T1,T22);bb{i,2}=LBss3(C,T1,T22)
            C=[];
        end
    else

        ee=[ee;a(c,:);a(c+1,:)]
%         bb=[bb;LBss2(a(c,:),T1,T22),LBss3(a(c,:),T1,T22);LBss2(a(c+1,:),T1,T22),LBss3(a(c+1,:),T1,T22)];
        bb{i,1}=LBss2(a(c,:),T1,T22);bb{i,2}=LBss3(a(c,:),T1,T22)
        i=i+1
        bb{i,1}=LBss2(a(c+1,:),T1,T22);bb{i,2}=LBss3(a(c+1,:),T1,T22)
    end
end