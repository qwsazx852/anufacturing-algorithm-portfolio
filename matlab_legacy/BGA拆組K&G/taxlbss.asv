            add=add+DC2(j);
        end
    end

    for j =1:length(d1)
       %% 加工成本%% remanufact
        n=0;
        for j1 =1:length(Remanufacturing)
            a=find(d1==Remanufacturing(j1));
            if a>1
                n=n+1;
            end
        end
        add=add+1.5.*n;%%加工係數

    end
